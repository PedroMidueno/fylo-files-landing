{
  "kind": "collectionType",
  "collectionName": "features",
  "info": {
    "singularName": "feaure",
    "pluralName": "features",
    "displayName": "Caracteristicas"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string",
      "minLength": 10,
      "maxLength": 60,
      "required": true
    },
    "Descripcion": {
      "type": "text",
      "minLength": 50,
      "maxLength": 200,
      "required": true
    },
    "Imagen": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Orden": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1
    }
  }
}
